<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- Moving Code backend has to be enabled. --> 
<wps:Execute service="WPS" version="1.0.0"
	xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1"
	xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsExecute_request.xsd">

	<ows:Identifier>de.tu-dresden.geo.gis.algorithms.test.echo</ows:Identifier>

	<wps:DataInputs>
		<wps:Input>
			<ows:Identifier>CALL</ows:Identifier>
			<wps:Data>
				<wps:ComplexData>
					Don't you talk back to me!
					
					Check out "So Much Drama in the PhD" at http://www.monzy.com/intro/drama_lyrics.html
				</wps:ComplexData>
			</wps:Data>
		</wps:Input>
	</wps:DataInputs>

	<wps:ResponseForm>
		<wps:RawDataOutput>
			<ows:Identifier>RESPONSE</ows:Identifier>
		</wps:RawDataOutput>
	</wps:ResponseForm>
</wps:Execute>