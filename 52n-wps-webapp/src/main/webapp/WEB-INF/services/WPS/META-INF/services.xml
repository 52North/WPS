	</operation>
	<operation name="describeData">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:describeData</actionMapping>
	</operation>
	<operation name="pauseJob">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:pauseJob</actionMapping>
	</operation>
	<operation name="resumeJob">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:resumeJob</actionMapping>
	</operation>
	<parameter name="messageReceiver.invokeOnSeparateThread" locked="false">false</parameter>
</service>