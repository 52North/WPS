<service name="WPS">
	<description>
		52n WPS
 	</description>
	<parameter name="ServiceClass" locked="false">
		org.n52.wps.server.WebProcessingServiceSoap
 	</parameter>
	<module ref="addressing" />
	<operation name="getCapabilities">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:getCapabilities</actionMapping>
	</operation>
		<operation name="describeProcess">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:describeProcess</actionMapping>
	</operation>
	<operation name="execute">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:execute</actionMapping>
	</operation>
	<operation name="getStatus">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:getStatus</actionMapping>
	</operation>
	<operation name="cancel">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:cancel</actionMapping>
	</operation>
	<operation name="deployProcess">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:deploy</actionMapping>
	</operation>
	<operation name="undeployProcess">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:undeploy</actionMapping>
	</operation>
	<operation name="pauseJob">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:pauseJob</actionMapping>
	</operation>
	<operation name="resumeJob">
		<messageReceiver
			class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver" />
		<actionMapping>urn:resumeJob</actionMapping>
	</operation>
</service>