<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:wps="http://www.opengis.net/wps/1.0.0" targetNamespace="http://www.opengis.net/wps/1.0.0" elementFormDefault="qualified" version="1.0.0 2010-02-03" xml:lang="en">
	<annotation>
		<appinfo>$Id: wpsGetAudit_response.xsd 2007-10-09 $</appinfo>
		<documentation>
			<description>This XML Schema Document encodes the WPS Execute operation response.</description>
			<copyright>
	WPS is an OGC Standard.
	Copyright (c) 2007,2010 Open Geospatial Consortium, Inc. All Rights Reserved.
	To obtain additional rights of use, visit http://www.opengeospatial.org/legal/ .
			</copyright>
		</documentation>
	</annotation>
	<!-- =============================================================
		includes and imports
	============================================================== -->
	<import namespace="http://www.opengis.net/ows/1.1" schemaLocation="../ows/1.1.0/owsAll.xsd"/>
	<include schemaLocation="common/ResponseBaseType.xsd"/>
	<include schemaLocation="common/ProcessBriefType.xsd"/>
	<include schemaLocation="wpsExecute_response.xsd"/>
	<include schemaLocation="common/ProcessInstanceType.xsd"/>
	<include schemaLocation="common/ProcessAuditType.xsd"/>
	<!-- =============================================================
		elements and types
	============================================================== -->
	<element name="GetAuditResponse">
		<annotation>
			<documentation>WPS GetAudit operation response. </documentation>
		</annotation>
		<complexType>
			<complexContent>
				<extension base="wps:ResponseBaseType">
					<sequence>
						<element name="Result">
							<annotation>
								<documentation>GetAudit request result.</documentation>
							</annotation>
							<complexType>
								<sequence>
									<element name="Message" type="string" minOccurs="0">
										<annotation>
											<documentation>Result message. Required if success is false.</documentation>
										</annotation>
									</element>
								</sequence>
								<attribute name="success" type="boolean" use="required"/>
							</complexType>
						</element>
						<element name="ProcessInstanceIdentifier" type="wps:ProcessInstanceType" minOccurs="0">
							<annotation>
								<documentation>Process instance identifier. Useful when success is false.</documentation>
							</annotation>
						</element>
						<element name="AuditTrace" type="wps:AuditTraceType" minOccurs="0">
							<annotation>
								<documentation>Required id success is true. </documentation>
							</annotation>
						</element>
					</sequence>
				</extension>
			</complexContent>
		</complexType>
	</element>
	<!-- =========================================================== -->
	<element name="InvokedTasks">
		<complexType>
			<sequence>
				<element name="Task" minOccurs="0" maxOccurs="unbounded">
					<complexType>
						<attribute name="timestamp" use="required"/>
						<attribute name="processIdentifier" use="required"/>
						<attribute name="processInstanceIdentifier" use="required"/>
						<attribute name="server" use="required"/>
					</complexType>
				</element>
			</sequence>
		</complexType>
	</element>
</schema>
